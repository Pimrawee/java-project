            <Label fx:id="nameStaff" layoutX="25.0" layoutY="21.0" prefHeight="21.0" prefWidth="438.0" text="nameStaff" textFill="WHITE">
               <font>
                  <Font name="Alternate Gothic No.2 BT" size="48.0" />
               </font>
            </Label>
            <Button fx:id="logout" layoutX="707.0" layoutY="33.0" mnemonicParsing="false" onAction="#handleToLogout" style="-fx-background-color: cae3cc;" text="Logout">
               <font>
                  <Font name="Bodoni MT" size="18.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
      <Label layoutX="330.0" layoutY="135.0" text="Add Guest">
         <font>
            <Font name="Alternate Gothic No.2 BT" size="48.0" />
         </font>
      </Label>
      <Button fx:id="back" layoutX="10.0" layoutY="555.0" mnemonicParsing="false" onAction="#handleToBack" style="-fx-background-color: #336600;" text="Back" textFill="WHITE">
         <font>
            <Font name="Bodoni MT Bold" size="18.0" />
         </font>
      </Button>
      <Label layoutX="185.0" layoutY="270.0" text="Room :">
         <font>
            <Font name="Alternate Gothic No.2 BT" size="24.0" />
