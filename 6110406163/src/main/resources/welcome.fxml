<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: #FFCCFF;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.WelcomeController">
   <children>
      <Label layoutX="160.0" layoutY="80.0" text="Welcome">
         <font>
            <Font name="Arial Black" size="96.0" />
         </font>
      </Label>
      <Button fx:id="administrator" layoutX="309.0" layoutY="275.0" mnemonicParsing="false" onAction="#handleToAdministratorLogin" prefHeight="46.0" prefWidth="182.0" style="-fx-background-color: #3366cc;" text="Administrator" textFill="WHITE">
         <font>
            <Font name="Cambria" size="24.0" />
         </font>
      </Button>
      <Button fx:id="staff" layoutX="309.0" layoutY="360.0" mnemonicParsing="false" onAction="#handleToStaffLogin" prefHeight="46.0" prefWidth="182.0" style="-fx-background-color: #336600;" text="Staff" textFill="WHITE">
         <font>
            <Font name="Cambria" size="24.0" />
         </font>
      </Button>
      <Button fx:id="profile" layoutX="711.0" layoutY="552.0" mnemonicParsing="false" onAction="#handleToProfile" style="-fx-background-color: #CC66FF;" text="Profile" textFill="WHITE">
         <font>
            <Font name="Cambria" size="18.0" />
         </font>
      </Button>
      <Button fx:id="manual" layoutX="621.0" layoutY="552.0" mnemonicParsing="false" onAction="#handleToManual" style="-fx-background-color: #CC66FF;" text="Manual" textFill="WHITE">
         <font>
            <Font name="Cambria" size="18.0" />
         </font></Button>
      <Button fx:id="guest" layoutX="309.0" layoutY="445.0" mnemonicParsing="false" onAction="#handleToGuestLogin" prefHeight="46.0" prefWidth="182.0" style="-fx-background-color: e76f51;" text="Guest" textFill="WHITE">
         <font>
            <Font name="Cambria" size="24.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
