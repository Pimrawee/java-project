      </Label>
      <Label layoutX="185.0" layoutY="320.0" text="Floor :">
         <font>
            <Font name="Alternate Gothic No.2 BT" size="24.0" />
         </font>
      </Label>
      <Label layoutX="185.0" layoutY="370.0" text="Building :">
         <font>
            <Font name="Alternate Gothic No.2 BT" size="24.0" />
         </font>
      </Label>
      <TextField fx:id="nameGuest" layoutX="315.0" layoutY="220.0" />
      <TextField fx:id="roomGuest" layoutX="315.0" layoutY="270.0" />
      <TextField fx:id="floorGuest" layoutX="315.0" layoutY="320.0" />
      <Label layoutX="185.0" layoutY="220.0" text="Name :">
         <font>
            <Font name="Alternate Gothic No.2 BT" size="24.0" />
         </font>
      </Label>
      <Button fx:id="addGuest" layoutX="350.0" layoutY="470.0" mnemonicParsing="false" onAction="#handleToAddGuest" prefHeight="46.0" prefWidth="87.0" style="-fx-background-color: #336600;" text="Add" textFill="WHITE">
         <font>
            <Font name="Bodoni MT Bold" size="24.0" />
         </font>
      </Button>
      <Label fx:id="error" alignment="CENTER" contentDisplay="CENTER" layoutX="169.0" layoutY="525.0" prefHeight="30.0" prefWidth="450.0" text="error" textAlignment="CENTER" textFill="#b51515" wrapText="true">
         <font>
            <Font name="Alternate Gothic No.2 BT" size="24.0" />
         </font>
      </Label>
      <Label layoutX="315.0" layoutY="370.0" prefHeight="29.0" prefWidth="15.0" text="1">
         <font>
            <Font name="Alternate Gothic No.2 BT" size="24.0" />
         </font>
      </Label>
      <Label layoutX="525.0" layoutY="270.0" text="Please enter 1-10 numbers.">
         <font>
            <Font name="Alternate Gothic No.2 BT" size="24.0" />
         </font>
      </Label>
      <Label layoutX="525.0" layoutY="320.0" text="Please enter 1-8 numbers.">
         <font>
            <Font name="Alternate Gothic No.2 BT" size="24.0" />
         </font>
      </Label>
